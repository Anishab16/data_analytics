num = int(input("Input a number: ")) 
if (num %2) == 0:  
   print(num, "is an even number")  
else:  
   print(num, "is an odd number")

num = int(input("Input a number: ")) 
if (num %4) == 0:
  print (num, "is a multiple of 4")
elif (num %2) == 0:  
   print(num, "is an even number") 
else:  
   print(num, "is an odd number")
